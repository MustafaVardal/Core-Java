package oop1;

public class Main {
	public static void main(String[] args) {
		// String message = "Price ratio: ";

		Product product1 = new Product();
		Product product2 = new Product();
		Product product3 = new Product();
		
		
			// These are coming from database in real life. 
						// set value
		product1.name= "Eureka WhirlWind Bagless Canister 2.5L Vacuum Cleaner";
		product1.productId = "123456";
		product1.unitPrice = 69.99;
		product1.year = 2024;
		product1.rating = 4.7;
		product1.unitsInStock = 100;
		product1.discount= 17.00;
		product1.imageUrl = "EurekaWhirlWind.svg";
		
		product2.name= "Eureka WhirlWind Bagless Canister 2.5L Vacuum Cleaner";
		product2.productId = "123456";
		product2.unitPrice = 69.99;
		product2.year = 2024;
		product2.rating = 4.7;
		product2.unitsInStock = 100;
		product2.discount= 17.00;
		product2.imageUrl = "EurekaWhirlWind.svg";
		
		product3.name= "Eureka WhirlWind Bagless Canister 2.5L Vacuum Cleaner";
		product3.productId = "123456";
		product3.unitPrice = 69.99;
		product3.year = 2024;
		product3.rating = 4.7;
		product3.unitsInStock = 100;
		product3.discount= 17.00;
		product3.imageUrl = "EurekaWhirlWind.svg";
		
		
		Product[] products = {product1, product2, product3};	
		
		for (Product product : products) {
			System.out.println(product.name);
		}
		
		System.out.println("");
						// get value
		System.out.println("Name: " + product1.name + "\nProduct Id: " + product1.productId +
				"\nPrice: " + product1.unitPrice + "\nRating: " + product1.rating +
