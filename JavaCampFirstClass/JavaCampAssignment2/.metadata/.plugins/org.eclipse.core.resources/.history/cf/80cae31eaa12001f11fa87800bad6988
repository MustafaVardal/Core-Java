
public class Variables {

	public static void main(String[] args) {
		
		final int numberStudent = 10;
		int visitorCount = 125;
		String sMessage = "Student Count : ";
		String vMessage = "Visitor Count : ";
	
		
		System.out.println(sMessage + numberStudent);
		System.out.println();
		System.out.println(vMessage + visitorCount);
		System.out.println();
		System.out.println("Student Count : ");

	}

}
