
public class Variables {

	public static void main(String[] args) {
		
		final int numberStudent = 10;
		int visitorCount = 125;
		
		System.out.println("Student Count : " + numberStudent);
		System.out.println();
		System.out.printf("Student Count : ", visitorCount);
		System.out.println("Student Count : ");

	}

}
