
public class CustomerManager {
	
	private Customer customer;
	
	
	
	public CustomerManager() {
		
	}

	public CustomerManager(Customer customer) {
		this.customer = customer;
	}
	
	public void Save() {
		System.out.println("Customer is saved." +customer.getId());
	}
	
	public void Delete() {
		System.out.println("Customer is deleted."+ customer.getId());
	}
}
